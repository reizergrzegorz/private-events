<h1><%= @user.email %></h1>

<p><strong>Created Events</strong></p>
<% @created_events.each do |event| %>
    <div>
        <p>Title: <%= event.title %></p> 
        <p>Location: <%= event.location %></p>
        <p>Date: <%= event.date %></p>
    </div><br>
<% end %>

<p><strong>Attended Events</strong></p>
<% @attended_events.each do |event| %>
    <div>
        <p><%= link_to event.title, event %></p>  
    </div><br>
<% end %>    

